package repository

import (
	"context"
	entity "github.com/brianshepanek/turnbull/_testing/output/domain/entity"
)

type fooRepository interface {
	Add(ctx context.Context, req entity.Foo) error
	Read(ctx context.Context, id string, req entity.Foo) error
	Browse(ctx context.Context, req entity.Foos) error
	Delete(ctx context.Context, id string, req entity.Foo) error
}
